<section class="section">
<div class="row">
  <div class="col-md-8">
    <h1>Role: <%= @role.name %></h1>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <p>
      <strong>Name:</strong>
      <%= @role.name %>
    </p>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <p>
      <strong>Slug:</strong>
      <%= @role.slug %>
    </p>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <p>
      <strong>Description:</strong>
      <%= @role.description %>
    </p>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <p>
      <strong>Created At:</strong>
      <%= @role.created_at %>
    </p>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <p>
      <strong>Updated At:</strong>
      <%= @role.updated_at %>
    </p>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <%= link_to 'Edit', edit_role_path(@role) %> |
    <%= link_to 'Back', roles_path %>
  </div>
</div>
</section>