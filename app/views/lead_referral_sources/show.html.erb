<p id="notice"><%= notice %></p>
<div class="row">
  <div class="col-md-12">
    <h1>Lead Referral Source: <%= @lead_referral_source.name %></h1>
  </div>  
</div>
<div class="row">
  <div class="col-md-12">
    <p>
      <strong>Name:</strong>
      <%= @lead_referral_source.name %>
    </p>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <p>
      <strong>Created At:</strong>
      <%= @lead_referral_source.created_at %>
    </p>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <p>
      <strong>Updated At:</strong>
      <%= @lead_referral_source.updated_at %>
    </p>
  </div>
</div>
<div class="row">
  <div class="col-md-4">
    <%= link_to 'Edit', edit_lead_referral_source_path(@lead_referral_source), class: "btn btn-small btn-primary" %>
    <%= link_to 'Back', lead_referral_sources_path, class: "btn btn-xs btn-info" %>
  </div>
</div>
