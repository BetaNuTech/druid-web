<div class="container">
  <div class="row">
    <div class="col">
      <h1>Edit Marketing Source</h1>
      <%= render 'form', marketing_source: @marketing_source %>
    </div>
  </div>
  <% if policy(@marketing_source).destroy? %>
    <div class="row">
      <div class="col-md-offset-10 col-md-2">
        <%= link_to('Delete', marketing_source_path(@marketing_source), method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure?'}) %>
      </div>
    </div>
  <% end %>
</div>
