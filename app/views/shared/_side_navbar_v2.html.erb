          <% if current_user.scheduled_actions.incomplete.any? %>
            <span class="badge bg-urgent">
              <%= current_user.scheduled_actions.incomplete.count %>
            </span>
          <% end %>
        <% end %>

        <%= link_to(messages_path, class: 'list-group-item' + nav_item_class(:messages)) do %>
          <svg class="svg-inline--fa fa-inbox" aria-hidden="true" focusable="false" data-prefix="fas" data-icon="inbox" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" data-fa-i2svg=""><path fill="currentColor" d="M447 56.25C443.5 42 430.7 31.1 416 31.1H96c-14.69 0-27.47 10-31.03 24.25L3.715 304.9C1.247 314.9 0 325.2 0 335.5v96.47c0 26.51 21.49 48 48 48h416c26.51 0 48-21.49 48-48v-96.47c0-10.32-1.247-20.6-3.715-30.61L447 56.25zM352 352H160L128 288H72.97L121 96h270l48.03 192H384L352 352z"></path></svg>
          Messages
        <% end %>

        <div id="nav-messages-div">
          <%= link_to(messages_path, class: 'list-group-item') do %>
            Me
            <% if current_user.messages.for_leads.relevant_to_leads.incoming.unread.any? %>
              <span class="badge bg-unread">
                <%= current_user.messages.for_leads.relevant_to_leads.incoming.unread.count %>
              </span>
            <% end %>
          <% end %>
          <% ( @current_property&.users&.active&.by_name_asc || [] ).each do |user| %>
            <%= link_to(messages_path, class: 'list-group-item') do %>
              <% next if user == current_user %>
              <%= user.name || user.email %>
              <% if user.messages.for_leads.relevant_to_leads.incoming.unread.any? %>
                <span class="badge bg-unread">
                  <%= user.messages.for_leads.relevant_to_leads.incoming.unread.count %>
                </span>
              <% end %>
            <% end %>
          <% end %>
        </div>
      </div>

