<div id="sidebar-wrapper">
  <div id="sidebar-heading">
    <div id="header-property-logo">
      <img src="/Bluesky-logo.svg" />
    </div>
  </div>
  <div id="sidebar-nav-links">
    <!--this area is populated by script-->

    <% if current_user %>
      <div id="sidebar-search">
        <span>SEARCH LEADS</span>
        <form action="/leads/search" method="get" class="form-inline my-2 my-lg-0" id="nav-search-form">
          <% if @current_property.present? %>
            <input type="hidden" name="lead_search[property_ids][]" value="<%=@current_property.id%>" />
          <% end %>
          <input type="hidden"name="lead_search[states][]" value="open" />
          <input type="hidden"name="lead_search[states][]" value="prospect" />
          <input type="hidden"name="lead_search[states][]" value="showing" />
          <input type="hidden"name="lead_search[states][]" value="application" />
          <input type="hidden"name="lead_search[states][]" value="approved" />
          <input type="hidden"name="lead_search[states][]" value="future" />
          <input type="hidden"name="lead_search[states][]" value="waitlist" />
          <input class="form-control mr-sm-2" type="text" name='lead_search[text][]' title="Search field" aria-label="Search field" />
          <button class="btn btn-outline-success my-2 my-sm-0" type="submit" title="Submit button to start the search" aria-label="Submit button to start the search"><i class="fa fa-search" style="font-size: 1.2em;"></i></button>
